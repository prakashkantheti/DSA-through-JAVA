class Main {
    public static void main(String[] args) {
   String name="jaya";
   int n=name.length();
      int sl=3;
      int sum=0;
      for(int i=0;i<=n-sl;i++){
          int j=i+sl-1;
            int   temp=0;
      for(int k=i;k<=j;k++){
          temp+=name.charAt(k);
         
      }System.out.println();
      sum=Math.max(temp,sum);
      }System.out.println("Heighest sum of an asccii "+sum);
    }
}
