class Main {
    public static void main(String[] args) {
       
 int[] arr={5,8,9,1,7};
 int n=arr.length;
 int sum=0;
 for(int i=0;i<n;i++){
     for(int j=i;j<n;j++) {
     int temp=0;
     for(int k=i;k<=j;k++){
         temp+=arr[k];
        
         
      
     }  sum=Math.max(sum,temp);
     } 
 }System.out.println(sum);
    }
}
